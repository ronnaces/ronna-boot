<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ronnaces.ronna.boot.system.modules.oauth2.client.mapper.SystemOauth2ClientMapper">

    <select id="findByClientId"
            resultType="com.ronnaces.ronna.boot.system.modules.oauth2.client.entity.SystemOauth2Client"
            parameterType="java.lang.String">
        select *
        from system_oauth2_client
        where client_id = #{clientId}
          and whether_delete = 0
    </select>
</mapper>
